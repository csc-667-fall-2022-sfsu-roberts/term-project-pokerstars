{
  "name": "pokerstars",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:dev:win": "SET \"NODE_ENV=development\" SET DEBUG=myapp:* & nodemon ./bin/www",
    "start:dev:mac": "NODE_ENV=development DEBUG=APP_NAME:* nodemon ./bin/www",
    "start": "node ./bin/www",
    "db:create:migration": "npx sequelize migration:generate --name",
    "db:migrate": "npx sequelize db:migrate",
    "db:rollback": "npx sequelize db:migrate:undo",
    "postinstall": "npx sequelize db:migrate"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "express-handlebars": "^6.0.6",
    "express-hbs": "^2.4.0",
    "express-session": "^1.17.3",
    "hbs": "^4.2.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pg": "^8.8.0",
    "pg-promise": "^10.12.1",
    "pgtools": "^0.1.1",
    "pug": "^3.0.2",
    "sequelize": "^6.25.1",
    "sequelize-cli": "^6.5.1",
    "touch": "^3.1.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
